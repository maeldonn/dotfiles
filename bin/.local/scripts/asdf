#!/usr/bin/env bash

# Switch to workspace
swaymsg -- 'bindsym $mod+1 workspace number 1'
swaymsg -- 'bindsym $mod+2 workspace number 2'
swaymsg -- 'bindsym $mod+3 workspace number 3'
swaymsg -- 'bindsym $mod+4 workspace number 4'
swaymsg -- 'bindsym $mod+5 workspace number 5'
swaymsg -- 'bindsym $mod+6 workspace number 6'
swaymsg -- 'bindsym $mod+7 workspace number 7'
swaymsg -- 'bindsym $mod+8 workspace number 8'
swaymsg -- 'bindsym $mod+9 workspace number 9'
swaymsg -- 'bindsym $mod+0 workspace number 10'

# Move focused container to workspace
swaymsg -- 'bindsym $mod+Shift+1 move container to workspace number 1'
swaymsg -- 'bindsym $mod+Shift+2 move container to workspace number 2'
swaymsg -- 'bindsym $mod+Shift+3 move container to workspace number 3'
swaymsg -- 'bindsym $mod+Shift+4 move container to workspace number 4'
swaymsg -- 'bindsym $mod+Shift+5 move container to workspace number 5'
swaymsg -- 'bindsym $mod+Shift+6 move container to workspace number 6'
swaymsg -- 'bindsym $mod+Shift+7 move container to workspace number 7'
swaymsg -- 'bindsym $mod+Shift+8 move container to workspace number 8'
swaymsg -- 'bindsym $mod+Shift+9 move container to workspace number 9'
swaymsg -- 'bindsym $mod+Shift+0 move container to workspace number 10'
